{
  "name": "vue-init",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode inner",
    "build": "vue-cli-service build",
    "build:dev": "npm run build --mode dev",
    "build:test": "npm run build --mode test",
    "build:release": "npm run build --mode release",
    "lint": "vue-cli-service lint",
    "release": "standard-version",
    "update:iconfont": "node script/update-iconfont"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.3.2",
    "element-ui": "^2.12.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-component": "^1.1.1",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "element-theme": "^2.0.1",
    "element-theme-chalk": "^2.12.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^4.0.0-beta.5",
    "lint-staged": "^9.4.2",
    "sass-loader": "^8.0.0",
    "sass-resources-loader": "^2.0.1",
    "standard-version": "^7.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix --ext .js,.vue src",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
